    <body>
        <div class="container">
            <!-- Codrops top bar -->
            <div class="codrops-top">

                <div class="clr"></div>
            </div><!--/ Codrops top bar -->
            <header>
                <h1>Pension And Life Insurance Webportal<br><span>Government of India</span></h1>
				<nav class="codrops-demos">

				</nav>
            </header>
            <section>
                <div id="container_demo" >
                    <!-- hidden anchor to stop jump http://www.css3create.com/Astuce-Empecher-le-scroll-avec-l-utilisation-de-target#wrap4  -->
                    <a class="hiddenanchor" id="toregister"></a>
                    <a class="hiddenanchor" id="tologin"></a>
                    <div id="wrapper">
                        <div id="login" class="animate form">
                            <form  action="www.google.com" autocomplete="on">
                                      <h1> OTP Verification </h1>
                                      <p>
                                          <label for="otp" class="uname" data-icon="u">OTP sent to your email id</label>
                                          <input id="usernamesignup" name="otp" required="required" type="text" placeholder="OTP HERE" />
                                      </p>

                                      <p class="signin button">
                        <input type="submit" value="Sign up"/>
                      </p>
                                      <p class="change_link">
                        Go Back Home
                        <a href="/home" class="to_register">Home</a>
                         Resend?
                        <a href="/login" class="to_register"> Resend </a>
                      </p>



                            </form>
                        </div>



                    </div>
                </div>
            </section>
        </div>
